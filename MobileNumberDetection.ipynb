   "source": [
    "import re \n",
    "def validate_phone_number(number): \n",
    "    if re.match(\"^(?:\\+91|0)?[6789]\\d{9}$\", number): \n",
    "        return True \n",
    "    else: \n",
    "        return False\n",
    "number = \"9254234529\"\n",
    "if validate_phone_number(number): \n",
    "    print(\"Valid Phone Number\") \n",
    "else: \n",
    "    print(\"Invalid Phone Number\")"
   ]
